{
  "name": "spryker/product-reviews-rest-api",
  "type": "library",
  "description": "ProductReviewsRestApi module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/glue-application": "^1.15.0",
    "spryker/glue-application-extension": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/product-review": "^1.0.0",
    "spryker/product-review-storage": "^1.0.0",
    "spryker/product-storage": "^1.3.0",
    "spryker/products-rest-api": "^2.8.0",
    "spryker/products-rest-api-extension": "^1.1.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
